<%= form_with(model: entry, local: true) do |form| %>
  <% if entry.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(entry.errors.count, "error") %> prohibited this entry from being saved:</h2>

      <ul>
      <% entry.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :idparcel %>
    <%= form.text_field :idparcel, id: :entry_idparcel %>
  </div>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :entry_name %>
  </div>

  <div class="field">
    <select name="desti_id">

    <% @destis.each do |category| %>

      <option value="<%= category.id %>"><%= category.name %></option>
		
    <% end %>

	
	
  </select>
  </div>
  

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
