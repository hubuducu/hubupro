var array_entry = []
<% @searchentries.each do |entry| %>
array_entry.push("ID: <%= entry.idparcel %>"+" "+"Nom: <%= entry.name %>"+" "+"Destination: <%= entry.desti.name %>");
<% end %>
var result = document.getElementById("result");
result.innerHTML = "";
array_entry.forEach((entry)=>{result.innerHTML += entry +"<br>"});


